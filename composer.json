{
  "name": "jascha030/cli",
  "description": "Service class for executing shell commands using symfony/process, heavily inspired by the CommandLine class in laravel/valet.",
  "keywords": [
    "php",
    "console",
    "CLI",
    "shell",
    "service",
    "symfony/console",
    "symfony/process"
  ],
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "license": "MIT",
  "autoload": {
    "files": [
      "includes/helpers.php"
    ],
    "psr-4": {
      "Jascha030\\CLI\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "autoload-dev": {
    "psr-4": {
      "Jascha030\\CLI\\Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/process": "^6.0",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.6",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "phpunit": "phpunit --config phpunit.xml",
    "format": "php-cs-fixer fix --config=.php-cs-fixer.dist.php"
  }
}
